{
  "manifest_version": 3,
  "name": "Meeting Notes",
  "version": "0.0.1",
  "description": "A chrome extension for transcribing meetings to get notes",
  "permissions": ["tabCapture", "activeTab", "downloads"],
  "icons": {
    "48": "icon.png"
  },
  "action": {
    "default_popup": "popup.html"
  },
  "web_accessible_resources": [
    {
      "resources": ["../src/permission/index.html", "../src/permission/requestPermission.ts"],
      "matches": ["*://*/*"]
    }
]
}
